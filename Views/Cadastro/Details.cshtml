@model RegistroViewModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Registros</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Nome do formulario
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NomeFormulario)
        </dd>
    </dl>
    @foreach (var dado in Model?.RegInfo)
    {
        <dl class="row">
            <dt class = "col-sm-2">
                @dado.Campo.Nome
            </dt>
            <dd class = "col-sm-10">
                @dado.Dado
            </dd>
        </dl>
    }
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
